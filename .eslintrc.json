{

    "extends": [
        "eslint:recommended"
    ],
    "env": {
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "plugins": [
    ],
    "overrides": [
        {
            "files": ["docs/**"],
            "rules": {
                "no-console": "off",
                "no-unused-vars": "off"
            }
        }
    ]
}